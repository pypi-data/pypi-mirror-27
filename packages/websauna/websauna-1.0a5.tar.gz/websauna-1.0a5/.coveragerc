[run]
branch = True
source = codecov

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if .debug:
    raise NotImplementedError
    assert
    if __name__ == .__main__.:


ignore_errors = True
omit =
    setup.py
    websauna/utils/orderedset.py
    websauna/tests/*
    websauna/system/http/*

