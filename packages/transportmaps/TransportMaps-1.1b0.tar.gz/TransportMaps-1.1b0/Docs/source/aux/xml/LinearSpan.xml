<?xml version="1.0"?>
<trimap 
    xmlns="TransportMaps"
    xmlns:st="SpectralToolbox"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="map.xsd"
    type="linspan" dim="5">
  <component id="*">
    <avars>
      <vars>*</vars>
    </avars>
    <approx>
      <linspan multidimtype="tensorized" spantype="total">
        <spanorder>
          <maxord coord="*">1</maxord>
        </spanorder>
        <spanbasis coord="*">
          <st:HermiteProbabilistsPolynomial/>
        </spanbasis>
      </linspan>
    </approx>
  </component>
</trimap>
