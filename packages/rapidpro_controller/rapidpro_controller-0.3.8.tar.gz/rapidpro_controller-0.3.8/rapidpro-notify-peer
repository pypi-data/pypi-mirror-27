from __future__ import (unicode_literals, absolute_import,
                        division, print_function)
import sys
import os

from rapidpro_controller import get_logger, get_remote_output, OTHER_SERVER

logger = get_logger(os.path.basename(__file__))


def main():
    return get_remote_output(['rapidpro-receive-notif'], OTHER_SERVER)


if __name__ == '__main__':
    sys.exit(main())
