<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!DOCTYPE document SYSTEM "rml.dtd">

<document filename="tag-ul-ol-li.pdf">

  <template>
    <pageTemplate id="main">
      <frame id="first" x1="1cm" y1="1cm" width="19cm" height="26cm"/>
    </pageTemplate>
  </template>

  <stylesheet>
    <initialize>
      <alias id="style.normal" value="style.Normal"/>
    </initialize>

    <listStyle
        name="MyList"
        bulletFontSize="50pt"
        bulletAlign='center'
        bulletFontName="Chalkduster"
        bulletColor="green"
        start="diamond"
        />
    <listStyle
        name="MyEnum"
        bulletFontName="Times-Roman"
        bulletFontSize="8pt"
        bulletAlign="center"
        bulletFormat="(%s)"
        bulletType="a"

        />
    <listStyle
        name="CustomBulletTextList"
        start="square"
        bulletFontName = "Times-Roman"
        bulletFontSize="50pt"
        bulletOffsetY="0pt"
        bulletDedent="100"
        leftIndent="100"
        />
    <listStyle
        name="OrderedList"
        bulletFontName="Chalkduster"
        bulletFontSize="50pt"
        bulletFormat="%s:"
        bulletDedent="100pt"
        leftIndent="100pt"
        />
    <listStyle
        name="OrdinalEnum"
        bulletFontName="Arial"
        bulletFontSize="8pt"
        bulletFormat="%s:"
        bulletType="R"
        bulletDedent="100pt"
        leftIndent="100pt"
        />
  </stylesheet>

  <story>
    <title>
      <font face="Courier">&lt;ul&gt;</font>,
      <font face="Courier">&lt;ol&gt;</font>, and
      <font face="Courier">&lt;li&gt;</font>
      Tags Demo
    </title>

    <ol bulletColor="orange" bulletFontName="Times-Roman">
      <li bulletColor="gray" bulletFontName="Helvetica"><para>Welcome to RML 1</para></li>
      <li>
        <ul bulletColor="red" bulletFontName="Times-Roman" bulletFontSize="8">
          <li value="disc" bulletFontName="Helvetica"><para>unordered 1</para></li>
          <li value="square" bulletColor="blue"><para>unordered 2</para></li>
          <li>
            <ul>
              <li><para>Unordered - another one</para></li>
              <li><para>Unordered - another one</para></li>
            </ul>
          </li>
          <li value="diamond" bulletColor="green"><para>unordered 3</para></li>
          <li value="rarrowhead" bulletColor="yellow"><para>unordered 4</para></li>
          <li><para>unordered 5</para></li>
          <li><para>unordered 6</para></li>
          <li value="disc" bulletColor="green"><para>unordered 3</para></li>
          <li bulletColor="yellow"><para>unordered 4</para></li>
        </ul>
      </li>
      <li><para>Welcome to RML 2</para></li>
      <li><para>Welcome to RML 3</para></li>
    </ol>

    <pb/>

    <spacer length="1.5in" />


    <ol bulletColor="orange" bulletFontName="Times-Roman">
      <li bulletColor="gray" bulletFontName="Chalkduster"><para>Item 1</para></li>
      <li><para>Item 2</para></li>
    </ol>

    <ul style="MyList">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ul>


    <NextPage/>

    <spacer length="1in" />


    <ol style="MyEnum">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>


    <spacer length="0.75in" />


    <ul style="CustomBulletTextList">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ul>


    <spacer length="0.8in" />

    <h1>Ordered List Bullet Types</h1>

    <spacer length="0.5in" />

    <ol style="OrderedList" bulletType="1">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

    <spacer length="0.3in" />

    <ol style="OrderedList" bulletType="a">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

    <spacer length="0.2in" />

    <ol style="OrderedList" bulletType="A">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

    <spacer length="0.5in" />

    <ol style="OrderedList" bulletType="i">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

    <spacer length="1in" />

    <ol style="OrderedList" bulletType="I">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

    <spacer length="1.5in" />

    <ol style="OrderedList" bulletType="l">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

    <spacer length="0.5in" />

    <ol style="OrderedList" bulletType="L">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

    <spacer length="1in" />

    <ol style="OrderedList" bulletType="o">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

    <spacer length="0.1in" />

    <ol style="OrderedList" bulletType="O">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

    <spacer length="0.05in" />

    <ol style="OrderedList" bulletType="r">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

    <spacer length="0.5in" />

    <ol style="OrderedList" bulletType="R">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

    <spacer length="0.5in" />
    <para>Use style to set bullet type.</para>
    <spacer length="0.1in" />

    <ol style="OrdinalEnum">
      <li><para>One</para></li>
      <li><para>Two</para></li>
      <li><para>Three</para></li>
    </ol>

  </story>
</document>
