            <dropzone x="58.0%" y="28.3%" w="11%" h="11%"><blank><image id="bingo_red"/></blank></dropzone>
            <dropzone x="86.0%" y="28.3%" w="11%" h="11%"><blank><image id="bingo_green"/></blank></dropzone>
