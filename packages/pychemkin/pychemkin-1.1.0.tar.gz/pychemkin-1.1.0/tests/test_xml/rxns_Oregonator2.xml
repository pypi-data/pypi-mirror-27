<?xml version="1.0"?>

<ctml>

    <phase>
        <speciesArray> X Y Z</speciesArray>
    </phase>

    <reactionData id="test_mechanism">

        <!-- reaction 01  -->
        <reaction reversible="no" type="Elementary" id="reaction01">
            <equation>2Y =] X</equation>
            <rateCoeff>
                <Constant>
                    <k>0.1</k>
                </Constant>
            </rateCoeff>
            <reactants>Y:1</reactants>
            <products>X:1</products>
        </reaction>

        <!-- reaction 02 -->
        <reaction reversible="no" type="Elementary" id="reaction02">
            <equation>X =] Z</equation>
            <rateCoeff>
                <Constant>
                    <k>0.2</k>
                </Constant>
            </rateCoeff>
            <reactants>X:1</reactants>
            <products>Z:1</products>
        </reaction>

        <!-- reaction 03 -->
        <reaction reversible="no" type="Elementary" id="reaction03">
            <equation>Z =] Y</equation>
            <rateCoeff>
                <Constant>
                    <k>0.1</k>
                </Constant>
            </rateCoeff>
            <reactants>Z:1</reactants>
            <products>Y:1</products>
        </reaction>
    </reactionData>

</ctml>
