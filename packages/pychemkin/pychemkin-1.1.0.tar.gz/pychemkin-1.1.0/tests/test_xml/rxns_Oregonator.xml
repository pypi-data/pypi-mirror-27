<?xml version="1.0"?>

<ctml>

    <phase>
        <speciesArray> X Y Z P Q</speciesArray>
    </phase>

    <reactionData id="test_mechanism">

        <!-- reaction 01  -->
        <reaction reversible="no" type="Elementary" id="reaction01">
            <equation>Y =] X</equation>
            <rateCoeff>
                <Constant>
                    <k>0.1</k>
                </Constant>
            </rateCoeff>
            <reactants>Y:1</reactants>
            <products>X:1</products>
        </reaction>

        <!-- reaction 02 -->
        <reaction reversible="no" type="Elementary" id="reaction02">
            <equation>X + Y =] P</equation>
            <rateCoeff>
                <Constant>
                    <k>1.6e+9</k>
                </Constant>
            </rateCoeff>
            <reactants>X:1 Y:1</reactants>
            <products>P:1</products>
        </reaction>

        <!-- reaction 03 -->
        <reaction reversible="no" type="Elementary" id="reaction03">
            <equation>X =] 2X + Z</equation>
            <rateCoeff>
                <Constant>
                    <k>6.0e+02</k>
                </Constant>
            </rateCoeff>
            <reactants>X:1</reactants>
            <products>X:2 Z:1</products>
        </reaction>
		
		<!-- reaction 04 -->
        <reaction reversible="no" type="Elementary" id="reaction04">
            <equation>2X =] Q</equation>
            <rateCoeff>
                <Constant>
                    <k>4.0e+07</k>
                </Constant>
            </rateCoeff>
            <reactants>X:2</reactants>
            <products>Q:1</products>
        </reaction>
		
		<!-- reaction 05 -->
        <reaction reversible="no" type="Elementary" id="reaction05">
            <equation>Z =] 0.75Y</equation>
            <rateCoeff>
                <Constant>
                    <k>1</k>
                </Constant>
            </rateCoeff>
            <reactants>Z:1</reactants>
            <products>Y:0.75</products>
        </reaction>
    </reactionData>

</ctml>
