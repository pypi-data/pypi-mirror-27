{
    "dashboard": {
        "new-dashboard": {
            "rows": [
                {
                    "collapse": false,
                    "editable": true,
                    "height": "250px",
                    "panels": [
                        {
                            "bars": false,
                            "datasource": "graphite",
                            "editable": true,
                            "error": false,
                            "fill": 1,
                            "lines": true,
                            "linewidth": 2,
                            "percentage": false,
                            "pointradius": 5,
                            "points": false,
                            "span": 12,
                            "stack": false,
                            "steppedLine": false,
                            "targets": [
                                {
                                    "target": "$hostname.Cpu.cpu_prct_used"
                                }
                            ],
                            "title": "no title (click here)",
                            "type": "graph",
                            "x-axis": true,
                            "y-axis": true
                        }
                    ],
                    "showTitle": false,
                    "title": "New row"
                }
            ],
            "templating": {
                "enabled": true,
                "list": [
                    {
                        "datasource": "graphite",
                        "includeAll": false,
                        "multi": false,
                        "name": "hostname",
                        "query": "node*",
                        "refresh": true,
                        "type": "query"
                    }
                ]
            },
            "timezone": "utc",
            "title": "New dashboard"
        }
    }
}
